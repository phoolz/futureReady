@model FutureReady.Models.School.Cohort

@{
    ViewData["Title"] = "Cohort Details";
}

<h1>Cohort Details</h1>

<div>
    <h4>Cohort</h4>
    <dl class="row">
        <dt class="col-sm-2">School</dt>
        <dd class="col-sm-10">@Model.School?.Name</dd>

        <dt class="col-sm-2">Graduation Year</dt>
        <dd class="col-sm-10">@Model.GraduationYear</dd>

        <dt class="col-sm-2">Graduation Month</dt>
        <dd class="col-sm-10">@Model.GraduationMonth</dd>
    </dl>
</div>

<p>
    <a class="btn btn-secondary" asp-action="Edit" asp-route-id="@Model.Id">Edit</a>
    <a class="btn btn-secondary" asp-action="Index">Back to List</a>
</p>

